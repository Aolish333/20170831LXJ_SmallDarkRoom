<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>
    <package name="basicDrive" extends="struts-default">
        <action name="calculator1" class="BasicCaculator">
            <result name="success">index.jsp</result>
        </action>
    </package>
    <package name="attributeDrive" extends="struts-default">
        <action name="calculator2" class="CalculateAction">
            <result name="success">attribute.jsp</result>
        </action>
    </package>
    <package name="eventDrive" extends="struts-default">
        <action name="calculator3" class="CaculatorAction">
            <result name="success">eventDriver.jsp</result>
        </action>
    </package>
</struts>